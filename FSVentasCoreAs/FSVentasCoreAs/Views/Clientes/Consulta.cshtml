@model IEnumerable<FSVentasCoreAs.Models.Clientes>

@{
    ViewData["Title"] = "Consulta";
}

<h2>Consulta</h2>

<table class="table">
    <thead>
        <tr>
                <th>
                    @Html.DisplayNameFor(model => model.Nombre)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Sexo)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Cedula)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Direccion)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Telefono)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Celular)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Fecha)
                </th>
            <th>
                @Html.DisplayNameFor(model => model.DistritosMunicipales)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Municipios)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Provincias)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Sectores)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Nombre)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Sexo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Cedula)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Direccion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Telefono)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Celular)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Fecha)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DistritosMunicipales.Nombre)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Municipios.Nombre)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Provincias.Nombre)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Sectores.Nombre)
            </td>
            <td>
                <a asp-action="Editar" asp-route-id="@item.ClienteId">Editar</a> |
                <a asp-action="Detalle" asp-route-id="@item.ClienteId">Detalle</a> |
                <a asp-action="Eliminar" asp-route-id="@item.ClienteId">Eliminar</a>
            </td>
        </tr>
}
    </tbody>
</table>
